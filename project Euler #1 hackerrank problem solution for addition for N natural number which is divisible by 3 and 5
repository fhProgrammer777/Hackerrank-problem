#include <iostream>
using namespace std;
void checknum(int a){
    int sum = 0;
    for(int i=1; i<a; i++){
        if(i%3==0 || i%5==0){
            sum+=i;
        }
    }
    cout<<sum;
}
int main(){ 
    int n;
    //cout<<"enter the number of time you wants to enter";
    cin>>n;
    int arr[n];
      //cout<<"enter the number for "<<n<<<times";
    for(int i = 0;i<n;i++){
      cin>>arr[i];
    }
    for(int b=0; b<n; b++){
        checknum(arr[b]);     //printing the returning value
           cout<<"\n";
    }
    return 0;
}
